# DexScreener API (ingen key behövs, men rate limits gäller)

# =============================================================================
# SYSTEM CONFIGURATION
# =============================================================================

# Loggningsnivå (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Auto-save intervall i sekunder
AUTO_SAVE_INTERVAL=300

# Emergency stop trigger (procent)
EMERGENCY_STOP_LOSS=0.10

# =============================================================================
# RISK MANAGEMENT
# =============================================================================

# Max risk per trade (procent av portfolio)
MAX_RISK_PER_TRADE=0.02

# Max total portfolio risk
MAX_PORTFOLIO_RISK=0.10

# Max drawdown innan emergency stop
MAX_DRAWDOWN=0.15

# Max daglig förlust
MAX_DAILY_LOSS=0.05

# =============================================================================
# TRADING PARAMETERS
# =============================================================================

# Tillåtna tokens för trading
ALLOWED_TOKENS=bitcoin,ethereum,solana,avalanche-2,polygon-pos

# Min confidence för trades
MIN_SIGNAL_CONFIDENCE=0.70

# Trading intervall i sekunder
TRADING_INTERVAL=300

# Max öppna positioner
MAX_OPEN_POSITIONS=5

# =============================================================================
# DEX CONFIGURATION
# =============================================================================

# Föredragen DEX
PREFERRED_DEX=uniswap_v3

# Max slippage för swaps
MAX_SLIPPAGE=0.005

# Min likviditet för trades
MIN_LIQUIDITY_USD=5000000

# =============================================================================
# NOTIFICATIONS (OPTIONAL)
# =============================================================================

# Discord webhook för notifications
DISCORD_WEBHOOK_URL=

# Telegram bot token och chat ID
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Email notifications
SMTP_SERVER=
SMTP_PORT=587
SMTP_USERNAME=
SMTP_PASSWORD=
NOTIFICATION_EMAIL=

# =============================================================================
# GOOGLE CLOUD WEB3 INTEGRATION
# =============================================================================

# Google Cloud Project ID (KRITISKT!)
GOOGLE_CLOUD_PROJECT_ID=

# Google Cloud region för resurser
GOOGLE_CLOUD_REGION=europe-west1

# Google Cloud zone för compute resurser
GOOGLE_CLOUD_ZONE=europe-west1-b

# =============================================================================
# WEB3 GATEWAY CONFIGURATION
# =============================================================================

# Aktivera Google Cloud Web3 Gateway
WEB3_GATEWAY_ENABLED=true

# Web3 Gateway endpoint (genereras automatiskt om inte satt)
WEB3_GATEWAY_ENDPOINT=

# Supportade chains för Web3 Gateway
SUPPORTED_CHAINS=ethereum,polygon,arbitrum,optimism,base,bsc

# =============================================================================
# BIGQUERY CONFIGURATION
# =============================================================================

# BigQuery location (EU, US, etc.)
BIGQUERY_LOCATION=EU

# BigQuery query timeout i sekunder
BIGQUERY_QUERY_TIMEOUT=300

# Max bytes billed per query
BIGQUERY_MAX_BYTES_BILLED=1000000000

# Use legacy SQL (false för standard SQL)
BIGQUERY_USE_LEGACY_SQL=false

# =============================================================================
# GOOGLE CLOUD SERVICE ACCOUNTS
# =============================================================================

# Service account key paths (för development)
GOOGLE_APPLICATION_CREDENTIALS=

# Trading bot service account
TRADING_BOT_SERVICE_ACCOUNT=trading-bot@GOOGLE_CLOUD_PROJECT_ID.iam.gserviceaccount.com

# Analytics service account
ANALYTICS_SERVICE_ACCOUNT=analytics-service@GOOGLE_CLOUD_PROJECT_ID.iam.gserviceaccount.com

# Monitoring service account
MONITORING_SERVICE_ACCOUNT=monitoring-service@GOOGLE_CLOUD_PROJECT_ID.iam.gserviceaccount.com

# =============================================================================
# GOOGLE CLOUD STORAGE
# =============================================================================

# GCS bucket för ML models
GCS_BUCKET_MODELS=GOOGLE_CLOUD_PROJECT_ID-ml-models

# GCS bucket för trading data
GCS_BUCKET_TRADING_DATA=GOOGLE_CLOUD_PROJECT_ID-trading-data

# GCS bucket för backups
GCS_BUCKET_BACKUPS=GOOGLE_CLOUD_PROJECT_ID-backups

# =============================================================================
# PUB/SUB CONFIGURATION
# =============================================================================

# Pub/Sub topic för trades
PUBSUB_TOPIC_TRADES=crypto-trades

# Pub/Sub topic för price updates
PUBSUB_TOPIC_PRICES=price-updates

# Pub/Sub topic för alerts
PUBSUB_TOPIC_ALERTS=trading-alerts

# =============================================================================
# VERTEX AI CONFIGURATION
# =============================================================================

# Vertex AI endpoint för trading predictions
VERTEX_AI_ENDPOINT=projects/GOOGLE_CLOUD_PROJECT_ID/locations/GOOGLE_CLOUD_REGION/endpoints/trading-predictor

# Vertex AI model för sentiment analysis
VERTEX_AI_MODEL_SENTIMENT=text-bison@001

# Vertex AI model för price prediction
VERTEX_AI_MODEL_PREDICTION=gemini-pro

# =============================================================================
# CLOUD FUNCTIONS CONFIGURATION
# =============================================================================

# Cloud Function för DEX oracle
CLOUD_FUNCTION_DEX_ORACLE=dex-oracle

# Cloud Function för market data processing
CLOUD_FUNCTION_MARKET_DATA=market-data-processor

# Cloud Function för notifications
CLOUD_FUNCTION_NOTIFICATIONS=notification-sender

# =============================================================================
# CLOUD RUN CONFIGURATION
# =============================================================================

# Cloud Run service för trading bot
CLOUD_RUN_SERVICE_TRADING_BOT=crypto-trading-bot

# Cloud Run service för analytics
CLOUD_RUN_SERVICE_ANALYTICS=trading-analytics

# Cloud Run service för risk management
CLOUD_RUN_SERVICE_RISK=risk-management

# =============================================================================
# RATE LIMITING
# =============================================================================

# Max retry attempts för API calls
MAX_RETRY_ATTEMPTS=3

# Base delay för exponential backoff (sekunder)
RETRY_BASE_DELAY=1.0

# Max delay för exponential backoff (sekunder)
RETRY_MAX_DELAY=60.0

# BigQuery rate limit (queries per minute)
BIGQUERY_RATE_LIMIT=100

# Web3 Gateway rate limit (calls per second)
WEB3_GATEWAY_RATE_LIMIT=10

# Vertex AI rate limit (calls per minute)
VERTEX_AI_RATE_LIMIT=60

# =============================================================================
# PERFORMANCE CONFIGURATION
# =============================================================================

# Batch size för BigQuery operations
BIGQUERY_BATCH_SIZE=1000

# Cache TTL för Firestore (sekunder)
CACHE_TTL_SECONDS=3600

# Connection pool size för databases
CONNECTION_POOL_SIZE=10

# =============================================================================
# DEVELOPMENT
# =============================================================================

# Debug mode
DEBUG=false

# Testnet mode (använd för testing)
TESTNET=true

# Base testnet RPC (om TESTNET=true)
BASE_TESTNET_RPC=https://sepolia.base.org

